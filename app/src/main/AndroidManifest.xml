<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.geoffsgeob">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher_cs125"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_cs125_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".NewGameActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <action android:name="android.intent.action.VIEW" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />
            </intent-filter>
        </activity>
        <activity android:name=".Help" />
        <activity android:name=".Settings" />
        <activity android:name=".About" />
        <activity android:name=".Bonuses" />
        <activity android:name=".GameActivity" />
        <activity android:name=".EndGame" />
        <service android:enabled="true" android:name=".BackgroundSoundService" />

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>